    "def F(u=1):\n",
    "    mesh=build_mesh(u=1)\n",
    "\n",
    "    def f_r(x):\n",
    "        return 2*(x[0]-1)*(x[0])*(3*x[1]**2-x[1]+1)+x[1]*(x[1]-1)\n",
    "        #return 2*(x[0]-1)*x[0]+2*(x[1]-1)*x[1]\n",
    "    \n",
    "    F=[]\n",
    "    \n",
    "    for x in mesh:\n",
    "        if x[0]==a1 or x[0]==b1 or x[1]==a2 or x[1]==b2:\n",
    "            #F.append(0)\n",
    "            F.append(f_r(x))\n",
    "        \n",
    "        else:\n",
    "            F.append(f_r(x))\n",
    "            \n",
    "    return np.array(F)"
   ]
  },
