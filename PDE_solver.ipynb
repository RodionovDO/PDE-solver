    "#Точное решение\n"
    "def U(u=1):\n",
    "    mesh=build_mesh(u=1)\n",
    "    \n",
    "    \n",
    "    def solution(x):\n",
    "\n",
    "        return x[0]*(x[0]-1)*x[1]*(x[1]-1)\n",
    "    \n",
    "    \n",
    "    U=[]\n",
    "    \n",
    "    for x in mesh:\n",
    "        if x[0]==a1 or x[0]==b1 or x[1]==a2 or x[1]==b2:\n",
    "            U.append(solution(x))\n",
    "        \n",
    "        else:\n",
    "            U.append(solution(x))\n",
    "        \n",
    "    return np.array(U)"
   ]
