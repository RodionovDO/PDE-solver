    "def calc_alpha(A, B, r_n):\n",
    "    B=np.linalg.inv(B)\n",
    "    p=np.matmul(B,r_n)\n",
    "    \n",
    "    numerator=np.dot(r_n, p)\n",
    "    denumerator=np.dot(np.matmul(A, p), p)\n",
    "    \n",
    "    alpha_n1=numerator/denumerator\n",
    "    \n",
    "    return alpha_n1"
   ]
