    "#Предобуславливатель для итерационного метода\n",
    "def laplas_B(n=N1, h=h1):\n",
    "    matrix_dim= n**2\n",
    "    \n",
    "    diag_1 = np.diag(np.array([4]*matrix_dim), 0)\n",
    "    diag_2 = np.diag(np.array([-1]*(matrix_dim - 1)), 1)\n",
    "    diag_3 = np.diag(np.array([-1]*(matrix_dim - 1)), -1)\n",
    "    diag_4 = np.diag(np.array([-1]*(matrix_dim - n)), n)\n",
    "    diag_5 = np.diag(np.array([-1]*(matrix_dim - n)), -n)\n",
    "    \n",
    "    i = n\n",
    "    while (i < matrix_dim - 1):\n",
    "        diag_2[i-1] = 0\n",
    "        diag_3[i] = 0\n",
    "        i += n\n",
    "    \n",
    "    return (diag_1 + diag_2 + diag_3 + diag_4 + diag_5) / h1**2"
   ]
  },
